alias ls='ls --color=auto'

g='\[\e[0;32m\]' # Green
b='\[\e[0;34m\]' # Blue
c='\[\e[0;36m\]' # Cyan
clr='\[\e[0m\]'    # Text Reset

PS1="$c┏ ⇐ $g( $b\w$g )\n$g┗ ⇒ $clr"
